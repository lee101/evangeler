{% extends 'templates/base.jinja2' %}

{% block headers %}

    {% set title = 'Evangeler Social Marketing - Money Back Guarantee' -%}
    {% set desc = 'Evangeler Helps Companies Create Viral Marketing Campaigns! See our Money Back Guarantee!' -%}


    <title>{{ title }}</title>
    <meta name="description"
          content="{{ desc }}">

    <meta name="keywords"
          content="crowdsourcing marketing, viral marketing, facebook marketing, marketing, social marketing">
    <meta property="og:title" content="{{ title }}">
    <meta property="og:url" content="{{ url }}">
    <meta property="og:image"
          content="http://www.evangeler.com/static/img/evangeler-logo128.png">
    <meta property="og:description"
          content="{{ desc }}">
{% endblock %}

{% block mainbody %}
    {% include "/templates/shared/refunds.jinja2" %}
{% endblock %}
