{% import "lib/templates/templates.jinja2" as lib %}
{% import "templates/macros.jinja2" as macros with context %}
<!doctype html>
<html>
<head>
    <meta charset="utf-8">

    {% block headers %}

    {% endblock %}

    {{ macros.headers() }}

    {{ lib.cssheaders() }}
    {{ macros.cssheaders() }}

    {% block css %}

    {% endblock %}
    {{ lib.jsheaders() }}

    {{ macros.jsheaders() }}
    <script>
        var fixtures = {};
        fixtures.STATUS = {{ fixtures.STATUS|safe }};
        fixtures.CONTESTS = {{ fixtures.CONTESTS|safe }};
        var CURR_CURSOR = '{{ next_page_cursor|safe }}';
    </script>
    {% block js %}

    {% endblock %}
</head>
<body>
{{ macros.fbstuff() }}
{{ macros.rollbar() }}
<div id="modal" class="modal fade" tabindex="-1" role="dialog"
     aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    <i class="fa fa-times close-modal"></i>
                </button>
            </div>
            <div class="modal-body">

            </div>
{#            <div class="modal-footer"></div>#}

        </div>
    </div>
</div>
<div class="mm-background">
    <div id="headerbody">
        {% include "/templates/shared/header.jinja2" %}
    </div>

    <div id="mainbody">
        {% block mainbody %}

        {% endblock %}
    </div>

    <div id="footerbody">
        {% include "/templates/shared/footer.jinja2" %}
    </div>

</div>

</body>
</html>
