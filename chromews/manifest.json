{
    "name": "Evangeler",
    "description": "Find numbers which work! Evangeler makes math fun! Beat the clock in this battle of the brains or create crazy combos! Evangeler Means Maths Madness!",
    "version": "1",
    "manifest_version": 2,
    "app": {
        "urls": [
            "*://www.evangeler.com/"
        ],
        "launch": {
            "web_url": "http://www.evangeler.com/"
        }
    },
    "icons": {
        "64": "cross-icon-multiplication-master-no-bg64.png",
        "128": "icon_128.png"
    },
    "permissions": [
        "unlimitedStorage",
        "notifications"
    ]
}
